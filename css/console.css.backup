#top-wrapper {
  position:relative;
  width:1200px;

}
#top-scroll {
  height:250px;
  overflow-y:scroll; 
  overflow-x:scroll;
  margin-top:20px;
  width:200%;
}
#top-wrapper table {
  width:200%;
    
}
#top-wrapper table * {
  background:#fff;
  color:black;
}


#top-wrapper table thead th {
  width:200%;
}


#console-top thead tr {
    display: block;
    width: 100%;
}

#console-top thead th:nth-child(3) {
    width: 100%;
}

$console-top tbody {
display:block;
}


#top-wrapper table thead th .top-text {
  position:absolute;   
  top:-21px;
  z-index:2;
  height:20px;
  width:35%;
  border:bottom 1px solid #000;
}

#console-top {
width:3400px;
table-layout: fixed;

}

.console-top-array {
overflow-y:hidden;
height:30px;
width:auto;
line-height:0;
}